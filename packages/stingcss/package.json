{
  "name": "@chargebee/cb-sting",
  "version": "0.0.43-alpha.3",
  "description": "Sting Design System",
  "main": "index.js",
  "files": [
    "/dist",
    "/theming"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chargebee/cb-react-dls.git"
  },
  "bugs": {
    "url": "https://github.com/chargebee/cb-react-dls/issues"
  },
  "homepage": "https://github.com/chargebee/cb-react-dls#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prejss-cli": "0.3.3",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build": "sh build.sh",
    "components": "postcss components/**/*.css --base ./ --dir dist",
    "merge:styled": "cat dist/components/*.css dist/components/*.css > dist/styled.css",
    "prejss": "prejss-cli ./dist/styled.css --format commonjs",
    "buildcss": "postcss dist/styled.css -o dist/styled.css",
    "global:build": "npm run components && npm run merge:styled && npm run buildcss && npm run prejss"
  },
  "keywords": [],
  "license": "ISC"
}
