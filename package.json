{
  "name": "builderio-ds",
  "version": "1.0.0",
  "description": "Builder.io compatible React design system components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:css && tsc",
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css --minify",
    "dev": "npm run dev:css & tsc --watch",
    "dev:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "npx http-server . -p 3001 -o",
    "storybook": "npm run build:css && storybook dev -p 6006",
    "build-storybook": "npm run build:css && storybook build",
    "dev-server": "npx live-server --port=3002 --entry-file=test.html"
  },
  "keywords": [
    "react",
    "design-system",
    "builder.io",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.20",
    "@storybook/addon-actions": "^7.6.20",
    "@storybook/addon-controls": "^7.6.20",
    "@storybook/addon-docs": "^7.6.20",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.20",
    "@storybook/react-vite": "^7.6.20",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.20",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^4.5.14"
  },
  "dependencies": {
    "@builder.io/react": "^3.0.0"
  }
}
